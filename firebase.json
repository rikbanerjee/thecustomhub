{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        
        "source": "/quant-alpha/**",
        "destination": "https://stock-analysis-103702095556.us-central1.run.app"
      },
      {
        
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        
        "source": "/quant-alpha",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache, no-store, must-revalidate"
          }
        ]
      }
    ]
  }
}